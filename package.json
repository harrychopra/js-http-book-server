{
  "name": "js-http-book-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "jest",
    "psql": "docker exec -it pg14 psql -d book_server",
    "create-db": "docker exec -it pg14 psql -c 'create database book_server;'",
    "drop-db": "docker exec -it pg14 psql -c 'drop database book_server;'",
    "migrate-up": "docker exec -i pg14 psql -d book_server < ./db/migrations/up.sql",
    "migrate-down": "docker exec -i pg14 psql -d book_server < ./db/migrations/down.sql",
    "seed": "node ./db/seed.js",
    "watch": "nodemon ./server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.3",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "pg-format": "^1.0.4"
  }
}
